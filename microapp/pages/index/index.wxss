page{
  background: #f5f8fa;
}
/* banner轮播图 */
.swiper_box {
  /*width: 100%;*/
  height: 187.5px;
  position: relative;
  margin-top: 16rpx;
}
swiper.swiper {
  height: 100%;
}
.swiper-item {
  display: block;
  height: 100%;
}
.slide-image {
  height: 100%;
  width: 100%;
  display: inline-block;
  overflow: hidden;
}

.swiper-box .wx-swiper-dots.wx-swiper-dots-horizontal{
  margin-bottom: 2rpx;
}
/* .swiper-box .wx-swiper-dots{
  position:relative;
  left: unset !important;
  right: 0;
} */
.swiper-box .wx-swiper-dot{
  width:40rpx;
  display: inline-flex;
  height: 10rpx;
  margin-left: 20rpx;
  justify-content:space-between;
}
.swiper-box .wx-swiper-dot::before{
  content: '';
  flex-grow: 1; 
  background: rgba(255,255,255,0.8);
  border-radius: 8rpx
}
.swiper-box .wx-swiper-dot-active::before{
  background:rgba(244,0,0,0.8);   
}

.imageCount{
  width: 80rpx;
  height: 80rpx;
  line-height: 78rpx;
  color: #ffffff;
  text-align: center;
  font-size: 30rpx;
  position: absolute;
  right: 26rpx;
  bottom:4rpx;
}



.authorAlert{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, .8);
  z-index: 9999;
}



.in_top_logo{
  padding: 0 20rpx;
  background: #fff;
}
.in_top_logo .in_logo{
  width: 300rpx;
  height: auto;
}
.in_top_address{
  float: right;
  color: #605e5e;
  padding-top: 10rpx;
  height: auto;
}
.in_top_address text{
  color: #f52640;
  text-decoration: underline;
}
.in_top_address image{
  width: 20rpx;
  vertical-align: middle;
  margin-right: 10rpx;
  margin-top: -3rpx;
}

.in_banner_box image{
  width: 100%;
  height: auto;
}
.in_img_tip{
  text-align: center;
  padding: 30rpx 0;

}
.in_img_tip image{
  width: 530rpx;
  height: auto;
}


/* 家庭保洁 */
.in_block_box{
  background: #fff;
  margin-bottom: 20rpx;
}
.in_block_title{
  padding: 30rpx 20rpx;
  line-height: 53rpx;
}
.shop_main_tt_line{
  font-size: 34rpx;
  font-weight: bold;
  display: inline-block;
  color: #161616;
}
.shop_main_tt_line text{
  display: inline-block;
  width: 5rpx;
  height: 38rpx;
  background: #ff0000;
  vertical-align: middle;
  margin-right: 16rpx;
  margin-top: -4rpx;
}
.in_more{
  float: right;
  margin-right: 10rpx;
  color: #898989;
}

.in_block_wrap{
  padding: 0 20rpx;
}
.in_block_list{
  width: 31.3%;
  display: inline-block;
  padding-right: 20rpx;
}
.in_block_list:last-child{
  padding-right: 0rpx;
}

.in_block_img{
  width: 100%;
  height: 220rpx;
  background: #f1f1f1;
  overflow: hidden;
  border-radius: 14rpx;
}
.in_block_img image{
  width: 100%;
  height: 220rpx;
}
.in_block_name{
  width: 100%;
  font-size: 31rpx;
  color: #161616;
  padding: 8rpx 0;
  margin-top: 10rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap; 
}
.in_block_money{
  color: #e94840;
  font-size: 30rpx;
  margin-bottom: 10rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap; 
  display: flex;
  align-items: center;
}
.in_block_money .goods_by{
  display: inline-block;
  border-radius: 5rpx;
  font-size: 21rpx;
  font-weight: normal;
  margin-left: 5rpx;
  margin-top: 2rpx;
  border: 1px solid #e94840;
  padding: 0 3rpx;
}
.in_block_address{
  color: #8d8b8b;
  margin-bottom: 20rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap; 
}
.in_block_address image{
  width: 26rpx;
  height: auto;
  margin-right: 10rpx;
  vertical-align: middle;
  margin-top: -3rpx;
}



/* 选择门店 */
.choiceStore-wrap{
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  z-index: 999;
  padding: 50rpx;
  padding-top: 100rpx;
}
.choicebackground{
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  background: rgba(0,0,0,0.5);
}
.store-boxwrap{
  position: relative;
  height: 100%;
}
.cancel{
  width: 26rpx;
  height:26rpx;
  position: absolute;
  bottom:-26rpx;
  left:50%;
  z-index: 999;
  margin-left: -13px;
}
.store-box{
  background: #fff;
  border-radius: 10rpx;
  overflow: hidden;
  height: 100%;
  position: relative;
  overflow-y: scroll;
}

.coll_top{
  background: #fff;
  position: relative;
  padding: 40rpx;
  margin-bottom: 20rpx;
}
.coll_top_left{
  display: inline-block;
  color: #2e2d2d;
  font-size: 30rpx;
  position: relative;
  padding-left: 50rpx;
  padding-right: 160rpx;
}
.coll_top_left image{
  width:40rpx;
  vertical-align: middle;
  margin-right: 20rpx;
}

.coll_top_right{
  display: inline-block;
  position: absolute;
  right: 20rpx;
  top: 20rpx;
  font-size: 24rpx;
}
.coll_color{
  color: #cd1b1b;
}
.coll_top_right .right_arrow{
  margin-left: 10rpx;
}

.coll_list{
  margin-bottom: 0;
  border-bottom: 1px solid #f5f8fa;
}
.coll_img_box{
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 50%;
  display: inline-block;
  vertical-align: middle;
  text-align: center;
  position: absolute;
  left: 0;
  top: 10rpx;
}
.coll_img_box image{
  width: 80rpx;
  margin: 0 auto;
}
.coll_text_box{
  display: inline-block;
  vertical-align: middle;
  padding-left: 20rpx;
}
.coll_text_weight{
  font-weight: bold;
}
.coll_text_box .coll_text_tip{
  font-size: 24rpx;
  color: #a2a0a0;
  margin-top: 10rpx;
}
.coll_text_right{
  top: 40%;
  color: #a2a0a0;
}