  bottom: 0;
  z-index: 9999;
  font-size: 30rpx;
}

.applybtn{
  padding: 0;
}
.applybtn button{
  background: transparent;
  color: #666;
  width: 100%;
  height: 100%;
  font-size: 30rpx;
}


.share_wrap{
  padding: 40rpx 35rpx;
}
.share_icon{
  display: inline-block;
  margin: 0 25rpx;
}
.share_icon>view{
  text-align: center;
  padding-top: 10rpx;
  color: #666;
  font-size: 28rpx;
}
.share_icon image{
  width: 110rpx;
}


/* 生成分享图 */
.shareprobox{
  width: 70%;
  background: #fff;
  border-radius: 20rpx;
  margin: 20% auto;
  padding: 20rpx;
  position: relative;
  text-align: left;
}

.sharepro{
  text-align:center;
  font-size:0;
}
.sharepro .shareprobox{
  vertical-align:middle;
  display:inline-block;
}
.sharepro:after{
  content:'';
  width:0;
  height:100%;
  display:inline-block;
  vertical-align:middle;
}

.sharepro_img image{
  width: 100%;
}
.sharepro_info{
  font-size: 32rpx;
  margin-top: 10rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
}
.sharepro_money{
  font-size: 46rpx;
  color: #f9233d;
  font-weight: bold;
  margin-top: 40rpx;
}
.sharepro_ym{
  font-size: 26rpx;
  color: #999;
  margin-top: 15rpx;
  margin-bottom: 50rpx;
  text-decoration:line-through;
}
.sharepro_code{
  position: absolute;
  right: 20rpx;
  bottom: 20rpx;
  border: 1rpx solid #dfdfdd;
  width: 190rpx;
  height: 190rpx;
  line-height: 190rpx;
  border-radius: 50%;
  overflow: hidden;
  text-align: center;
}
.sharepro_cancel{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: -50rpx;
  right: 0rpx;
  color: #fff;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 32rpx 32rpx;
}

.sharepro_btn{
  width: 100%;
  position: absolute;
  left: 0;
  bottom: -100rpx;
  text-align: center;
}
.sharepro_btn button{
  width: 200rpx;
  height: 80rpx;
  border-radius: 10rpx;
  background: #ff9800;
  color: #fff;
  margin: 0 auto;
  font-size: 30rpx;
}

.goods_exprice{
  font-size: 24rpx;
  color: #969494;
  padding-right: 20rpx;
  vertical-align: middle;
}