  /* display: flex; */
  /* height: 100rpx; */
  align-items: center;
  padding: 0 10rpx;
}
.thvl{
  padding-right: 20rpx;
}
.thvr{
  text-align: right;
  /* justify-content:right; */
  /* padding-left: 20rpx; */
}




/* 申请加入弹框 */
.Applyalert{
  background: rgba(0, 0, 0, 0.6);
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  z-index: 9998;
}
.applywrap{
  background: #fff;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 9999;
  font-size: 30rpx;
}
.applytitle{
  text-align: center;
  line-height: 80rpx;
  position: relative;
}
.applyinp{
  height: 100rpx;
  line-height: 100rpx;
  border-top: 1rpx solid #dfdfdd;
  position: relative;
  padding-left: 160rpx;
  padding-right: 20rpx;
}
.applyinp text{
  position: absolute;
  left: 20rpx;
  top: 0;
  display: block;
}
.applyinp input{
  width: 100%;
  height: 100%;
}
.getcode{
  min-width: 140rpx;
  position: absolute;
  right: 20rpx;
  top: 20rpx;
  background: #3594ff;
  border-radius: 10rpx;
  color: #fff;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 24rpx;
  padding: 0 20rpx;
}

.applybtn{
  padding: 0;
}
.applybtn button{
  background: #ff2741;
  color: #fff;
  width: 100%;
  height: 100%;
}

.applytwoBtn{
  padding: 0;
}
.applytwoBtn button{
  width: 100%;
  height: 100%;
  float: left;
  border: 1px solid #ff2741;
  color: #ff2741;
}
.applytwoBtn button:last-child{
  border: 1px solid #ff2741;
  background: #ff2741;
  color: #fff;
}


.cardRadiobox{
  max-height: 600rpx;
  overflow-y: auto;
}
.cardradiolist{
  padding: 20rpx;
}
.cardradiolist:nth-child(2n+1){
  background: rgba(0, 0, 0, 0.01)
}

.addbankcard{
  position: absolute;
  right: 15rpx;
  top: 0;
  font-size: 24rpx;
  color: #333;
}

.succicon{
  text-align: center;
  padding: 60rpx 0 14rpx 0;
}
.succicon image{
  width: 200rpx;
  margin: 0 auto;
}
.succtt{
  font-size: 34rpx;
  text-align: center;
  padding: 15rpx 10rpx;
}
.succtt text{
  color: #ff7901;
}
.succtips{
  text-align: center;
  font-size: 24rpx;
  color: #999;
  padding: 15rpx 10rpx 30rpx 10rpx;
}
