/* pages/mine/collection/collection.wxss */
page{
  background: #f5f8fa;
}


.share_money_top{
  background: #fff;
  width: 92%;
  margin: 0 auto;
  margin-top: 36rpx;
  border-radius: 15rpx;
}
.share_m_list{
  width: 25%;
  text-align: center;
  border-right: 1px solid #eceded;
}
.share_m_list:last-child{
  border-right: 0;
}

.share_m_title{
  text-align: center;
  font-size: 34rpx;
  font-weight: bold;
  color: #3e3c3c;
  padding: 36rpx 0;
}
.share_m_now{
  padding: 0 40rpx;
  font-size: 30rpx;
  margin-top: 10rpx;
}
.share_m_now text{
  color: #bc2828;
  font-size: 38rpx;
}
.share_now_get{
  float: right;
  line-height: 28rpx;
  display: inline-block;
  padding:10rpx 24rpx;
  background: #bc2828;
  border-radius: 6rpx;
  color: #fff;
  font-size: 24rpx;
}
.share_block_flex{
  padding: 70rpx 0 40rpx 0;
}
.share_m_list view{
  padding: 7rpx 0;
  color: #7c7a7a;
}
.share_m_list .shzre_money{
  font-size: 32rpx;
  color: #3e3c3c;
}



.share_money_list{
  padding-bottom: 40rpx;
}
.share_list_title{
  text-align: center;
  font-size: 32rpx;
  color: #3e3c3c;
  padding: 40rpx 0 10rpx 0;
  font-weight: 800;
}
.table {
  color: #7c7a7a;
  padding-right: 20rpx;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 1.5rem;
  align-items: center;
  padding-top: 10rpx;
  border-bottom: 1px solid #f4f5f5;
}
.tr:last-child{
  border-bottom: 0;
}
.thead{
  border-bottom: 0;
  height: 2rem;
}
.td {
  width:40%;
  justify-content: center;
  text-align: center;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space:nowrap; 
  padding: 0 10rpx;
}
.th {
  width: 40%;
  text-align: center;
  /* justify-content: center; */
